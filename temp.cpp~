#include<stdio.h>
#include<stdlib.h>
#include<string.h>
typedef struct tree
{
	char ch;
	struct tree* l,*r;
}Tree;
char pre[27],in[27];
void create(Tree* t,int li,int ri,int lp,int rp)
{printf("%c %d\n",pre[lp],lp);
	t=(Tree *)malloc(sizeof(tree));
	t->ch=pre[lp];
	t->l=t->r=NULL;
	int pos=li; 
	while(in[pos++]!=pre[lp]);
	int lenL=pos-li;
	if(pos>li) create(t->l,li,pos-1,lp+1,pos-1);
	//printf("test");
	//if(pos<ri) create(t->r,pos+1,ri,pos+1,rp);
}
void postorder(Tree *t)
{
	if(t!=NULL)
	{
		postorder(t->l);
		postorder(t->r);
		putchar(t->ch);
	}
}

int main()
{
	freopen("data.txt","r",stdin);
	while(scanf("%s %s",pre,in)!=EOF)
	{//puts(pre);puts(in); 
		Tree* t;
		create(t,0,strlen(pre),0,strlen(in));
		postorder(t);
		putchar('\n');
	}
	
	return 0;
}
